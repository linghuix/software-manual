   mdi gui_utl_alert_box_1 &
      type="info" &
      text="Modified type not stored yet. Click 'APPLY' and try again!"
! Invoke a panel for every associated classic motion
elseif cond=( db_object_count(DB_DEPENDENTS(.try.JOINT_1, "motion"))  >  0 )
   for var=.gui.joint_cremod.add_motions_button.motion_pnt obj=(eval( DB_DEPENDENTS(.try.JOINT_1, "motion" ) ))
      int com com="constraint mod motion" init=(eval( .gui.joint_cremod.add_motions_button.motion_pnt.object_value ))
   end
! Search for 'general motions' dependent on this joint. If there is none, create
! one. If there is one, offer it for modification
else
   var set var=.gui.joint_cremod.add_motions_button.general_motion_cnt int=0
   var set var=.gui.joint_cremod.add_motions_button.general_motion_pnt obj=(none)
   for var=.gui.joint_cremod.add_motions_button.variable_pnt obj=(eval(DB_DEPENDENTS(.try.JOINT_1, "variable" ) ))
      if cond=( db_type( .gui.joint_cremod.add_motions_button.variable_pnt.object_value.parent ) == "general_motion" )
         var set var=.gui.joint_cremod.add_motions_button.general_motion_cnt &
            int=(eval( .gui.joint_cremod.add_motions_button.general_motion_cnt + 1 ))
         var set var=.gui.joint_cremod.add_motions_button.general_motion_pnt &
            ind=(eval( .gui.joint_cremod.add_motions_button.general_motion_cnt )) &
            obj=(eval( .gui.joint_cremod.add_motions_button.variable_pnt.object_value.parent ))
      end
   end
   if cond=( .gui.joint_cremod.add_motions_button.general_motion_cnt == 0 )
      mdi default_instance create general_motion &
         model=.try  &
         name="general_motion_1"  &
         i_marker=MARKER_3  &
         j_marker=MARKER_4  &
         constraint=.try.JOINT_1  &
         show_dbox=yes
   elseif cond=( .gui.joint_cremod.add_motions_button.general_motion_cnt == 1 )
      int dia dis dia=.gui.general_motion_cremod &
         par=(eval( .gui.joint_cremod.add_motions_button.general_motion_pnt.object_value ))
   else
      mdi gui_utl_alert_box_1 &
         type="Error" &
         text="More than one general_motion associated with ."
      end
